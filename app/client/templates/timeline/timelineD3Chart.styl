@import 'client/templates/timeline/timeline.import.styl'

.timeline
  path
  	stroke-width 1
  	fill none
  .data1
  	stroke colorA
  .data2
  	stroke colorB
  .axis
    text
      font-size 8px
      fill colorC
    shape-rendering crispEdges
    .x
      line, .minor, path
        stroke colorC
    .y
      line, path
        fill none
        stroke colorC
      .axisRight text
  	    fill colorA
      .axisLeft text
  	    fill colorB
